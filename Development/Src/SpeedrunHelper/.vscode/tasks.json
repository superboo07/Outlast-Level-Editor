    "version": "2.0.0",
    "tasks": [
        {
            "label": "Compile Speedrun helper",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "type": "process",
            "command": "Compile.bat",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": false,
                "clear": true
            },
            "problemMatcher": "$msCompile"
        }
    ]
}